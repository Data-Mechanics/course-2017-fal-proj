from math import sin, cos, sqrt, atan2, radians

def latlondist(latone, lonone, lattwo, lontwo):

    R = 6373.0

    lat1 = radians(latone)
    lon1 = radians(lonone)
    lat2 = radians(lattwo)
    lon2 = radians(lontwo)

    dlon = lon2 - lon1
    dlat = lat2 - lat1

    a = sin(dlat / 2)**2 + cos(lat1) * cos(lat2) * sin(dlon / 2)**2
    c = 2 * atan2(sqrt(a), sqrt(1 - a))

    distance = R * c

    return distance

x = latlondist(42.347597, -71.102686, 42.349507, -71.104838)
print x